> resp <- initVariableFrame(GlaucomaM[,"Class",drop = FALSE], trafo = NULL, response = TRUE)
> ls <- new("LearningSample", inputs = inp, responses = resp,
+           weights = rep(1, inp@nobs), nobs = nrow(GlaucomaM), 
+           ninputs = inp@ninputs)
> tm <- ctree_memory(ls, TRUE)
> ctrl <- ctree_control()
